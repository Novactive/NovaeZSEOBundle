{
    "name": "novactive/ezseobundle",
    "description": "Novactive eZ SEO Bundle is an eZ Platform bundle for SEO simplications. metas, sitemaps, robots.txt, etc.",
    "keywords": [
        "seo",
        "ezplatform",
        "novactive",
        "metas",
        "robots.txt",
        "sitemap"
    ],
    "homepage": "https://github.com/Novactive/NovaeZSEOBundle",
    "type": "ezplatform-bundle",
    "authors": [
        {
            "name": "Novactive",
            "homepage": "https://www.novactive.us",
            "email": "dir.tech@novactive.com"
        },
        {
            "name": "Sebastien Morel",
            "homepage": "https://github.com/Plopix",
            "email": "morel.seb@gmail.com"
        }
    ],
    "license": [
        "MIT"
    ],
    "require": {
        "php": "^7.2",
        "ext-dom": "*",
        "ext-pdo": "*",
        "ext-json": "*",
        "ezsystems/ezpublish-kernel": "^7.1"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.3",
        "phpmd/phpmd": "^2.6",
        "friendsofphp/php-cs-fixer": "^2.12",
        "phpunit/phpunit": "^7.1"
    },
    "autoload": {
        "psr-4": {
            "Novactive\\Bundle\\eZSEOBundle\\": "bundle",
            "Novactive\\Bundle\\eZSEOBundle\\Tests\\": "tests/Tests"
        }
    },
    "extra": {
        "thanks": {
            "name": "ezsystems/ezplatform",
            "url": "https://github.com/ezsystems/ezplatform"
        }
    }
}
